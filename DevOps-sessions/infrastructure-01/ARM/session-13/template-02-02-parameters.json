{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "region": {
            "value": "westeurope"
        },
        "resourceGroup": {
            "value": "infrastructure-03"
        },
        "privateEndpointName": {
            "value": "private-endpoint-01"
        },
        "privateLinkName": {
            "value": "private-link-01"
        },
        "appName": {
            "value": "damwar-app-01"
        },
        "planName": {
            "value": "damwar-app-plan-01"
        },
        "vnetName": {
            "value": "infrastructure-03-vnet"
        },
        "subnetName": {
            "value": "agents"
        },
        "planTier": {
            "value": "S1"
        },
        "nodesInWebFarm": {
            "value": 1
        },
        "dockerHubImage": {
            "value": "nginx:alpine"
        },
        "vnetAddressSpace": {
            "value": "10.0.0.0/16"
        },
        "subnets": {
            "value": [
                {
                    "name": "agents",
                    "properties": {
                        "addressPrefix": "10.0.1.0/24"
                    }
                },
                {
                    "name": "jumpbox",
                    "properties": {
                        "addressPrefix": "10.0.2.0/24"
                    }
                }
            ]
        }
    }
}